;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA   dns1.{URL}. root.{URL}. (
		      {DATA}      	; Serial
			   3600		; Refresh
			  86400		; Retry
			  86400		; Expire
			   600 )	; Negative Cache TTL
;
@	IN	NS	{URL}.
@	IN	NS	www.{URL}.
@	IN	NS	dns.{URL}.
@	IN	NS	dns1.{URL}.
dns1	IN	A	{IPA}
@	IN	A	{IPA}
@	IN	A	{IP}
dns	IN	A	{IP}
www	IN	A	{IP}
